{
  "name": "node-develhope",
  "version": "1.0.0",
  "description": "Create a new Node.js project and install the figlet package (https://www.npmjs.com/package/figlet). Write a script that uses the function from this package to output some text based art (the figlet package README has a 'Quick Start' section). Run the script with Node.js.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "build:dev": "tsc --watch",
    "start": "node --enable-source-maps server.js",
    "start:dev": "nodemon --watch /",
    "dev": "npm run build && concurrently 'npm:build:dev' 'npm:start:dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d-94k/node-develhope.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/d-94k/node-develhope/issues"
  },
  "homepage": "https://github.com/d-94k/node-develhope#readme",
  "dependencies": {
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "figlet": "^1.5.2"
  },
  "devDependencies": {
    "@tsconfig/node18": "^1.0.1",
    "@types/express": "^4.17.15",
    "concurrently": "^7.6.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3",
    "typescript": "^4.9.4"
  }
}
